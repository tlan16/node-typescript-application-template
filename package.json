{
  "name": "nodejs-application",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env --no-warnings=ExperimentalWarning --import @swc-node/register/esm-register src/main.ts",
    "dev": "node  --env-file=.env --no-warnings=ExperimentalWarning --import @swc-node/register/esm-register --watch src/main.ts",
    "build": "del dist && tsc --project tsconfig.build.json",
    "test": "node --env-file=.env --no-warnings=ExperimentalWarning --import @swc-node/register/esm-register --test **/*.spec.ts",
    "lint": "eslint",
    "check-types": "tsc --noEmit",
    "prepare": "husky"
  },
  "main": "src/main.ts",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@stylistic/eslint-plugin-js": "^2.1.0",
    "@stylistic/eslint-plugin-ts": "^2.1.0",
    "@swc-node/register": "^1.9.1",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint__js": "^8.42.3",
    "@types/lint-staged": "^13.3.0",
    "@types/node": "^20.12.12",
    "@types/user-agents": "^1.0.4",
    "del-cli": "^5.1.0",
    "eslint": "^9.4.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  },
  "dependencies": {
    "pino": "^9.1.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": "^22.2.0"
  },
  "packageManager": "pnpm@9.3.0"
}
